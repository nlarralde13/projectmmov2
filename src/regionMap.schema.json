{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["seed", "dimensions"],
  "properties": {
    "seed": {
      "type": "string"
    },
    "dimensions": {
      "type": "object",
      "required": ["width", "height"],
      "properties": {
        "width": {
          "type": "integer",
          "minimum": 1
        },
        "height": {
          "type": "integer",
          "minimum": 1
        }
      }
    },
    "regions": {
      "type": "array",
      "items": {
        "type": "object"
      }
    },
    "worldName": {
      "type": "string"
    },
    "metadata": {
      "type": "object"
    }
  },
  "additionalProperties": false
}
