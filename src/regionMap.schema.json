{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^(NW|N|NE|W|C|E|SW|S|SE)$": {
      "type": "object",
      "properties": {
        "label": { "type": "string" },
        "landmassSize": {
          "type": "string",
          "enum": ["large", "medium", "small"]
        }
      },
      "required": ["label", "landmassSize"],
      "additionalProperties": false
    }
  }
}
